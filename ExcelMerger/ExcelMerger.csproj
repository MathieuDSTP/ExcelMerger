<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>  
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <PublishSingleFile>true</PublishSingleFile>
    <SelfContained>true</SelfContained>
    <!-- <PublishTrimmed>true</PublishTrimmed> -->
    <Authors>Mathieu Decelles St-Pierre</Authors>
    <Company>Mathieu Decelles St-Pierre</Company>
    <Copyright>Â© Mathieu Decelles St-Pierre. All rights reserved.</Copyright>
    <Product>ExcelMerger</Product>
    <Description>ExcelMerger is a console application that merges specific sheets from multiple Excel files into a single Excel file.</Description>
    <Version>1.0.0</Version>
    <license>MIT</license> 
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.9.1" />
    <PackageReference Include="NPOI" Version="2.7.1" />
  </ItemGroup>
</Project>
